<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="gui.SellerListController">
    <children>
        <Label text="Seller Registration">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <ToolBar prefHeight="40.0" prefWidth="200.0">
            <items>
                <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="New"/>
            </items>
        </ToolBar>
        <TableView fx:id="SellerTableView" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="tableColumnId" prefWidth="60.0" text="Id"/>
                <TableColumn fx:id="tableColumnName" prefWidth="250.0" text="Name"/>
                <TableColumn fx:id="tableColumnEdit" minWidth="0.0" prefWidth="60.0" sortable="false"/>
                <TableColumn fx:id="tableColumnDelete" prefWidth="70.0"/>
            </columns>
        </TableView>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets left="5.0" top="5.0"/>
    </padding>
</VBox>
